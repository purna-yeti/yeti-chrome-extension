{
	"manifest_version": 2,
	"name": "Yeti Hunt",
	"description": "Yeti Chrome Extension for auto bookmarking",
	"version": "0.01",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["js/lib/jquery-3.5.1.min.js", "js/app/content.js"]
		}
	],
	"background": {
		"scripts": ["js/lib/jquery-3.5.1.min.js", "js/app/background.js"]
	},
	"browser_action": {
		"default_popup": "views/popup.html"
	},
	"permissions": [
         "tabs",
		 "bookmarks"
  	]	
}